{
  "$schema": "node_modules/wrangler/config-schema.json",
  "main": "app/index.ts",
  "name": "logosophe-email",
  "compatibility_date": "2024-12-30",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "vars": {
    "EMAIL_FROM_ADDRESS": "info@anchorwrite.net",
    "EMAIL_TO_ADDRESS": "info@logosophe.com"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "logosophe",
